<p id="notice"><%= notice %></p>
<h1>Delete Page</h1>
<h2>Post</h2>

<%= form_for(@collectbook, :method => 'delete') do |f|%>
    <p>
    <strong>Title:</strong>
    <%= @collectbook.title %>
    </p>

    <p>
    <strong>Author:</strong>
    <%= @collectbook.author %>
    </p>

    <p>
    <strong>Price:</strong>
    <%= @collectbook.price %>
    </p>

    <p>
    <strong>Time:</strong>
    <%= @collectbook.time %>
    </p>
    <h5>Are you sure you want to delete this post? </h5>

    <div class="form-buttons">
        <%= f.submit("Delete Subject") %>
    </div>
    <%= link_to 'Edit', edit_collectbook_path(@collectbook) %> |
    <%= link_to 'Back', collectbooks_path %>

<% end %>
